{{#if patient}}
<!-- Name -->
<div id='name' style='display: inline-block;'>
  <label>First Name</label>

  <!-- First -->
  <input id='patient-first-name' type='text' name='patient[]info[]firstname' style="width: 250px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='50' data-value='["patient", "firstname"]' value='{{patient.firstname}}' readonly>
</div>
<div id='name' style='display: inline-block;'>
  <label>Last Name</label>
  <!-- Last -->
  <input id='patient-last-name' type='text' name='patient[]info[]lastname' style="width: 250px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='50' data-value='["patient", "lastname"]' value='{{patient.lastname}}' readonly>
</div>

<!-- Gender -->
<div style='display: inline-block; margin-left: 20px;'>
  <label>Gender</label>

  <input type='text' name='patient[]info[]gender' style="width: 54px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='6' data-value='patient.gender' value='{{patient.gender}}' readonly> 
</div><br />

<!-- SSN -->
<div style='display: inline-block;'>
  <label>Social Security #</label>
  <input class='SSN' type='text' name='patient[]info[]ssn' style="width: 108px;" pattern='^\d{3}\-\d{2}\-\d{4}$' maxlength='11' data-value='patient.ssn' value='{{patient.ssn}}' readonly>
</div>

<!-- DOB -->
<div style='display: inline-block;'>
  <label>Date of Birth</label>
  <input class='DOB' type='text' name='patient[]info[]dob' style="width: 120px;" pattern='^\d{2}\/\d{2}\/\d{4}$' maxlength='10' data-value='patient.dob' value='{{patient.dob}}' readonly>
</div>

<!-- Age -->
<div style='display: inline-block;'>
  <label>Age</label>
  <input type='text' name='patient[]info[]age' style="width: 30px;" pattern='\d{1,3}' maxlength='3' data-value='patient.age' value='{{patient.age}}' readonly>
</div>

<!-- Marital Status -->
<div style='display: inline-block;'>
  <label>Marital Status</label>
  <input type='text' name='patient[]info[]age' style="width: 85px;" pattern='\d{1,3}' maxlength='3' data-value='patient.maritalstatus' value='{{patient.maritalstatus}}' readonly>
</div><br />

  {{#if info.spouse}}

    <!-- Spouse Info -->
    <div style='width: 750px;'>
      <hr class='form-line'/><br />
        <h6>Spouse's Information</h6>

        <!-- Name -->
        <div id='name' style='display: inline-block;'>
          <label>Name</label>

          <!-- First -->
          <input id='patient-first-name' type='text' name='patient[]info[]firstname' style="width: 250px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='50' data-value='info.spouse.firstname' value='{{info.spouse.firstname}}' readonly>

          <!-- Last -->
          <input id='patient-last-name' type='text' name='patient[]info[]lastname' style="width: 250px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='50' data-value='info.spouse.lastname' value='{{info.spouse.lastname}}' readonly>
        </div>

        <!-- Gender -->
        <div style='display: inline-block; margin-left: 20px;'>
          <label>Gender</label>

          <input type='text' name='patient[]info[]gender' style="width: 54px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='6' data-value='info.spouse.gender' value='{{info.spouse.gender}}' readonly> 
        </div><br />

        <!-- SSN -->
        <div style='display: inline-block;'>
          <label>Social Security #</label>
          <input type='text' name='patient[]hstore[]spouse[]ssn' style="width: 108px;" pattern='^\d{4}$' maxlength='4' data-value='info.spouse.ssn' value='{{info.spouse.ssn}}' readonly>
        </div>

        <!-- DOB -->
        <div style='display: inline-block;'>
          <label>Date of Birth</label>
          <input class='DOB' type='text' name='patient[]hstore[]spouse[]dob' style="width: 120px;" pattern='^\d{2}\/\d{2}\/\d{4}$' maxlength='10' data-value='info.spouse.dob' value='{{info.spouse.dob}}' readonly>
        </div>
      <hr class='form-line'/>
    </div><br />
  {{/if}}

  {{#if info.address}}

    <!-- Mailing Address -->
    <div style='display: inline-block'>
      <label>Street Address</label>
      <!-- Street Address -->
      <input type='text' name='patient[]hstore[]address[]street' style="display: inline; width: 300px; text-transform:capitalize;" data-value'info.address.street' value='{{info.address.street}}' readonly>
    </div>
    
    <div style='display: inline-block'>
      <label>City</label>
      <!-- City -->
      <input type='text' name='patient[]hstore[]address[]city' style="display: inline; width: 200px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='50' data-value'info.address.city' value='{{info.address.city}}' readonly>
    </div>
      
    <div style='display: inline-block'>
      <label>State</label>
      <!-- State -->
      <div style='display: inline-block'>
        <input id='state' name='patient[]hstore[]address[]state' style="text-transform: capitalize; width: 86px;" type="text" data-provide="typeahead" data-items="4" data-source='["Alabama","Alaska","Arizona","Arkansas","California","Colorado","Connecticut","Delaware","Florida","Georgia","Hawaii","Idaho","Illinois","Indiana","Iowa","Kansas","Kentucky","Louisiana","Maine","Maryland","Massachusetts","Michigan","Minnesota","Mississippi","Missouri","Montana","Nebraska","Nevada","New Hampshire","New Jersey","New Mexico","New York","North Dakota","North Carolina","Ohio","Oklahoma","Oregon","Pennsylvania","Rhode Island","South Carolina","South Dakota","Tennessee","Texas","Utah","Vermont","Virginia","Washington","West Virginia","Wisconsin","Wyoming"]' pattern='^[a-zA-Z]+$' maxlength='11' data-value'info.address.state' value='{{info.address.state}}' readonly/>
      </div>
    </div>

    <div style='display: inline-block'>
      <label>Zip Code</label>

      <!-- Zip Code -->
      <input type='text' name='patient[]hstore[]address[]zip' style="display: inline; width: 55px;" pattern='^\d{5}$' maxlength='5' data-value'info.address.zip' value='{{info.address.zip}}' readonly>
    </div><br />

  {{else}}
  <p>There is no address for this patient</p>
  {{/if}}

  <!-- Email -->
  <div style='display: inline-block;'>
    <label>Email Address</label>
    <input id='email' type='text' name='patient[]info[]email' style="display: inline; width: 300px;" pattern='^[0-9a-zA-Z&#34;&#38;&#39;&#92;&#33;&#36;&#42;&#45;&#61;&#94;&#96;&#124;&#126;&#35;&#37;&#43;&#47;&#63;&#95;&#123;&#125;]+@{1}[0-9a-zA-Z&#34;&#38;&#39;&#92;&#33;&#36;&#42;&#45;&#61;&#94;&#96;&#124;&#126;&#35;&#37;&#43;&#47;&#63;&#95;&#123;&#125;]+\.[a-zA-Z]{2,4}$' maxlength='40' data-value='patient.email' value='{{patient.email}}' readonly>
  </div>

  <!-- Primary Phone-->
  <div style='display: inline-block;'>
    <label>Primary Phone</label>
    <input class='USphone' type='text' name='patient[]info[]primaryphone' style="display: inline; width: 105px;" pattern='^\d{3}-\d{3}-\d{4}$' maxlength='12' data-value='patient.primaryphone' value='{{patient.primaryphone}}' readonly>
  </div>

  <div style='display: inline-block;'>
    <label>Work Phone</label>
    <!-- Work Phone-->
    <input class='USphone' type='text' name='patient[]info[]workphone' style="display: inline; width: 105px;" pattern='^\d{3}-\d{3}-\d{4}$' maxlength='12' data-value='patient.workphone' value='{{patient.workphone}}' readonly>
  </div>

  <div style='display: inline-block;'>
    <label>Cell Phone</label>
    <!-- Cell Phone-->
    <input id='cellphone-profile' class='USphone' type='text' name='patient[]info[]cellphone' style="display: inline; width: 105px;" pattern='^\d{3}-\d{3}-\d{4}$' maxlength='12' data-value='patient.cellphone' value='{{patient.cellphone}}' readonly>
  </div><br />

  <!-- Email Messages Switch -->
  <div style='display: inline-block; margin-right: 25px;'>
    <label>Gender</label>
    <input type='text' name='patient[]info[]emailmessages' style="width: 66px; text-transform:capitalize;" data-value='patient.emailmessages' value='{{patient.emailmessages}}' readonly> 
  </div>

  <!-- Email Messages Switch -->
  <div style='display: inline-block; margin-right: 25px;'>
    <label>Gender</label>
    <input type='text' name='patient[]info[]textmessages' style="width: 66px; text-transform:capitalize;" data-value='patient.textmessages' value='{{patient.textmessages}}' readonly> 
  </div>

{{else}}
<p>There is no contact data for this patient</p>
{{/if}}

{{#if info.emergencycontact}}
          
  <hr class='form-line'/><br />
  <h5>Emergency Contact</h5>

  <!-- Emergency Contact Name -->
  <div style='display: inline-block;'>
    <label>Name</label>

    <!-- First Name -->
    <input type='text' name='patient[]hstore[]emergencycontact[]firstname' style="width: 250px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='50' placeholder='First'>

    <!-- Last Name -->
    <input type='text' name='patient[]hstore[]emergencycontact[]lastname' style="width: 250px; text-transform:capitalize;" pattern='^[a-zA-Z]+$' maxlength='50' placeholder='Last'>
  </div><br />

   <!-- Emergency Contact Number -->
  <div style='display: inline-block;'>
    <label>Contact Number</label>
    <input class='USphone'type='text' name='patient[]hstore[]emergencycontact[]number' style="display: inline; width: 105px;" pattern='^\d{3}-\d{3}-\d{4}$' maxlength='12' placeholder="Primary Phone">
  </div>

  <!-- Emergency Contact Relationship -->
  <div class='dropdown' style='width: 135px;'>
    <label>Relationship</label>
    <select name="patient[]hstore[]emergencycontact[]relationship" class=" form-dropdown select-block mbl" style='display: inline-block'>
      <optgroup label="Choose Best Fit">
        <option value="friend">Friend</option>
        <option value="guardian">Guardian</option>
        <option value="grandparent">Grandparent</option>
        <option value="sibling">Sibling</option>
        <option value="spouse">Spouse</option>
      </optgroup>
     </select>
  </div>
{{/if}}

          <hr class='form-line'/><br />
          <h5>Occupation</h5>

